<Project>
  
  <Import Project="dependencies.props" />

  <PropertyGroup>
    <MinVerMinimumMajorMinor>2.2</MinVerMinimumMajorMinor>
    <MinVerDefaultPreReleasePhase>preview8</MinVerDefaultPreReleasePhase>
  </PropertyGroup>
  
  <PropertyGroup>
    <Authors>Prospa Engineering</Authors>
    <Company>Prospa Advance Pty Limited</Company>
    <Copyright>Â© $([System.DateTime]::UtcNow.ToString(yyyy)) Prospa Advance Pty Limited. All rights reserved.</Copyright>
    <PackageIcon>prospa60x60.png</PackageIcon>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <GenerateAssemblyProductAttribute>false</GenerateAssemblyProductAttribute>
    <GenerateNeutralResourcesLanguageAttribute>false</GenerateNeutralResourcesLanguageAttribute>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <LangVersion>latest</LangVersion>
    <RepositoryType>git</RepositoryType>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <EmbedUntrackedSources>true</EmbedUntrackedSources>
    <CodeAnalysisRuleSet>..\..\Prospa.ruleset</CodeAnalysisRuleSet>
  </PropertyGroup>

  <ItemGroup>
    <None Include="$(MSBuildThisFileDirectory)..\prospa60x60.png" Pack="true" Visible="false" PackagePath="" />
  </ItemGroup>
  
  <ItemGroup>
    <AdditionalFiles Include="..\..\stylecop.json" Link="stylecop.json" />		
  </ItemGroup>
  
  <ItemGroup>
    <PackageReference Include="StyleCop.Analyzers" Version="$(StyleCopAnalyzersVersion)" PrivateAssets="All" />
  </ItemGroup>
  
</Project>